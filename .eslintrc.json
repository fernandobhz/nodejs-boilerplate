{
    "env": {
        "es2020": true,
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended",
        "plugin:jest"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
      "import/prefer-default-export": 0
    },
    "ignorePatterns": "dist",
    "plugins": ["jest"]
}
